# 主要API配置（GPT-4）
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.free.fastapi.pro/v1
OPENAI_MODEL=gpt-4

# 降级API配置（SiliconFlow Qwen）
SILICONFLOW_API_KEY=your_siliconflow_api_key_here
SILICONFLOW_BASE_URL=https://api.siliconflow.cn/v1
SILICONFLOW_MODEL=Qwen/Qwen2.5-32B-Instruct

# LLM 通用配置
LLM_TEMPERATURE=0.7

# 服务器配置
BACKEND_HOST=0.0.0.0
BACKEND_PORT=8000

# CORS 配置 (用逗号分隔多个域名)
CORS_ORIGINS=http://localhost:3000,http://192.168.38.226:3000

# GitLab 配置 (用于仓库管理)
GITLAB_TOKEN=your_gitlab_personal_access_token_here
GITLAB_URL=https://gitlab.com

# AI Agent 自动化配置
CLAUDE_CLI_PATH=/path/to/claude/cli
TEMP_FILES_CLEANUP_HOURS=24